<module fritzingVersion="0.7.0b.02.01.5801" moduleId="d63bf67236bb974ced6c30c1d9f152a9_18" referenceFile="Humidity_and_Temperature_Sensor_RHT03.fzp">
 <author>Andre van Ooijen</author>
 <title>MPX2010 pressure sensor</title>
 <label>MPX2010</label>
 <date>zo apr. 7 2013</date>
 <tags>
  <tag>Pressure</tag>
 </tags>
 <properties>
  <property name="family">Motorola</property>
  <property name="variant">variant 1</property>
  <property name="sensing element">Silicon Pressure Sensors</property>
  <property name="power supply">3.3-10 DC</property>
