<?xml version='1.0' encoding='UTF-8'?>
<module fritzingVersion="0.4.3b.09.26.4458" moduleId="LilyPad Vibe Motor">
    <author>J.A. Korten - JKSOFT Educational</author>
    <title>Lilypad Vibe Motor</title>
    <label>Lilypad Vibe</label>
    <date>2010-11-18</date>
    <tags>
        <tag>Lilypad</tag>
        <tag>Vibe</tag>
        <tag>Motor</tag>
    </tags>
    <properties>
        <property name="family">microcontroller board (lilypad)</property>
        <property name="type">Vibe Board</property>
    </properties>
    <description>Description: Apply 5V and be shaken by this small, but powerful vibration motor. Works great as an physical indicator without notifying anyone but the wearer. (See: http://www.sparkfun.com/products/8468)</description>
    <views>
        <iconView>
            <layers image="breadboard/Lilypad_Vibe_Motor.svg">
                <layer layerId="icon"/>
            </layers>
        </iconView>
        <breadboardView>
            <layers image="breadboard/Lilypad_Vibe_Motor.svg">
                <layer layerId="breadboard"/>
            </layers>
        </breadboardView>
        <schematicView>
            <layers image="schematic/Lilypad_Vibe_Motor.svg">
                <layer layerId="schematic"/>
            </layers>
        </schematicView>
        <pcbView>
            <layers image="pcb/Lilypad_Vibe_Motor.svg">
                <layer layerId="copper0"/>
                <layer layerId="silkscreen"/>
                <layer layerId="copper1"/>
            </layers>
        </pcbView>
    </views>
    <connectors>
        <connector type="female" id="connector18" name="Supply">
            <description>Pin to 5V</description>
            <views>
                <breadboardView>
                    <p svgId="connector18" layer="breadboard"/>
                </breadboardView>
                <schematicView>
                    <p svgId="connector18pin" layer="schematic" terminalId="connector18terminal"/>
                </schematicView>
                <pcbView>
                    <p svgId="connector18" layer="copper1"/>
                    <p svgId="connector18" layer="copper0"/>
                </pcbView>
            </views>
        </connector>
        <connector type="female" id="connector1" name="GND">
            <description>Pin to GND</description>
            <views>
                <breadboardView>
                    <p svgId="connector1" layer="breadboard"/>
                </breadboardView>
                <schematicView>
                    <p svgId="connector1pin" layer="schematic" terminalId="connector1terminal"/>
                </schematicView>
                <pcbView>
                    <p svgId="connector1" layer="copper1"/>
                    <p svgId="connector1" layer="copper0"/>
                </pcbView>
            </views>
        </connector>
    </connectors>
</module>
